---
title: Portfolio api
permalink: /portfolio.json
sitemap: false
---
	
[
	{% for portfolio in site.portfolio %}
    {
      "title": "{{ portfolio.title }}",
      "project_url": "{{ portfolio.project_url }}",
      "date": "{{ portfolio.date | "%e %b, 20%y" }}",
      "completed_on": "{{ portfolio.completed_on | "%e %b, 20%y" }}",
      "image": "{{ portfolio.image }}",
      "categories": "{{ portfolio.categories | jsonify }}",
      "technologies": "{{ portfolio.technologies | jsonify }}",
      "galleryImages": "{{ portfolio.galleryImages | jsonify }}",
	  "content": "{{ portfolio }}",
      "url": "{{ portfolio.url }}",
      "slug": "{{ portfolio.slug }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]